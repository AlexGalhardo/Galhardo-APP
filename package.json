    "scripts": {
        "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
        "start": "nodemon dist/server.js",
        "pm2": "pm2 start dist/server.js -i max --name galhardoapp --watch",
        "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
        "seed": "npx prisma db seed",
        "migrate": "npx prisma migrate dev",
        "start:dev": "ts-node-dev --inspect --transpile-only --watch node_modules --respawn src/server.ts",
        "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts"
    },
